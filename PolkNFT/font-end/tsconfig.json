{
   "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
         {
            "name": "next"
         }
      ],
      "paths": {
         "@/*": ["./*"]
      }
   },
   "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      ".next/types/**/*.ts",
      "pages/_document.tsx",
      "pages/nftCollection/NFTModel.jsx",
      "utils/context/index.js",
      "utils/connectWallet/ConnectWallet.tsx",
      "role/createNFT/createnft.js",
      "components/Banner.tsx",
      "role/sellNFT/sellNFT.js",
      "role/unlistNFT/index.js",
      "pages/nftDetail/[id]/index.jsx",
      "(models)/user.js",
      "utils/provider/provider.tsx"
   ],
   "exclude": ["node_modules"]
}
